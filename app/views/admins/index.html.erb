<div class='col-md-8 col-sm-12 col-xs-12 col-md-offset-2'>
  <% @admins.each do |admin| %>
      <div class='col-md-2 col-sm-2 col-xs-2'>
          <%= admin.username %>
      </div>

      <div class='col-md-2 col-sm-2 col-xs-2'>
          <%= admin.email %>
      </div>

      <div class='col-md-2 col-sm-2 col-xs-2'>
          <%= admin.sign_in_count %>
      </div>

      <div class='col-md-2 col-sm-2 col-xs-2'>
          <%= admin.last_sign_in %>
      </div>

      <% if current_admin.super_admin || current_admin == admin %>
          <div class='col-md-4 col-sm-4 col-xs-4'>
            <%= link_to "Edit Admin", edit_admin_path(admin) %>
            <% if current_admin.super_admin %>
              <%= link_to "Unactivate this admin", admin_path(admin), method: :delete %>
            <% end %>
          </div>
      <% end %>
  <% end %>
</div>
<% if current_admin.super_admin %>
  <%= link_to "Set New Admin", new_admin_path %>
<% end %>
